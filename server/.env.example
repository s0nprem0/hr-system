# Example environment variables for the server
# Copy this file to `.env` and fill values for your environment

# MongoDB connection string
MONGO_URI=mongodb://127.0.0.1:27017/hr-system

# JWT signing key (REQUIRED in production)
JWT_KEY=your_jwt_secret_here

# Port for the server
PORT=5000

# URL of the client (for CORS)
CLIENT_URL=http://localhost:5173
